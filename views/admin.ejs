<table class="table">
  <thead class="thead-dark">
    <h3 style="text-align: center; margin-bottom: 30px">
      Employees Attendance
    </h3>
    <tr>
      <th>S.No</th>
      <th>Name</th>
      <th>Date</th>
      <th>Attendance</th>
    </tr>
  </thead>
  <% if(userData.length!=0){ var i=1; userData.forEach(function(data){ %>
  <tr>
    <td><%=i; %></td>
    <td><%=data.name %></td>
    <td><%=data.date %></td>
    <td><%=data.attendance %></td>
    <td><a href="/users/edit/<%=data.id%>">Edit</a></td>
    <td><a href="/users/delete/<%=data.id%>">Delete</a></td>
  </tr>
  <% i++; }) %> <% } else{ %>
  <tr>
    <td colspan="7">No Data Found</td>
  </tr>
  <% } %>
</table>
<button type="submit" class="btn btn-primary mb-3" style="margin-top: 30px">
  <a href="/" style="color: #fff; text-decoration: none">Back </a>
</button>
